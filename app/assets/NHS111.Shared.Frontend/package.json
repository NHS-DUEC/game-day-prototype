{
  "name": "nhs111.shared.frontend",
  "version": "1.0.0",
  "private": "true",
  "scripts": {
    "enginestrict": "node scripts/engine-strict.js",
    "preinstall": "npm run enginestrict",
    "prebuild": "npm run enginestrict",
    "prebuild-production": "npm run enginestrict",
    "prelint": "npm run enginestrict",
    "preupdate-template-debug-comments": "npm run enginestrict",
    "pre test": "npm run enginestrict",
    "test": "jest src",
    "test:update": "jest src -u",
    "build": "rimraf dist && npm-run-all -p tasks:* webpack",
    "watch": "rimraf dist && npm-run-all -p tasks:* webpack-watch",
    "build-production": "rimraf dist && npm-run-all tasks:* webpack-production",
    "tasks:nhs-assets": "cpx \"node_modules/nhsuk-frontend/packages/assets/**/*.*\" dist",
    "tasks:email-assets": "cpx \"src/email/**/*.*\" dist/email",
    "svgo": "svgo -rf dist -o dist --config ./scripts/svgo.config.js",
    "posttasks": "npm run svgo",
    "webpack": "webpack --config webpack.dev.js",
    "webpack-watch": "npm run webpack -- --watch",
    "webpack-production": "webpack --config webpack.production.js",
    "update-template-debug-comments": "node scripts/update-template-debug-comments.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "useragent-regexp": "browserslist-useragent-regexp --allowHigherVersions --ignoreMinor --ignorePatch"
  },
  "engines": {
    "npm": "8",
    "node": "16"
  },
  "browserslist": [
    ">0.25% in GB",
    "not ie 11",
    "not op_mini all",
    "not iOS<12"
  ],
  "dependencies": {
    "@babel/core": "7.17.10",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "7.17.10",
    "@googlemaps/js-api-loader": "^1.15.2",
    "accessible-autocomplete": "^2.0.4",
    "autosize": "^5.0.1",
    "babel-loader": "8.2.5",
    "browserslist-useragent-regexp": "^3.0.2",
    "core-js": "^3.22.4",
    "cpx": "1.5.0",
    "css-loader": "6.7.1",
    "cssnano": "5.1.14",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "file-loader": "6.2.0",
    "glob": "8.0.1",
    "imagemin-cli": "7.0.0",
    "jest": "^28.0.3",
    "jest-environment-jsdom": "^28.0.2",
    "jest-serializer-html": "^7.1.0",
    "mini-css-extract-plugin": "2.6.0",
    "mkdirp": "^1.0.4",
    "nhsuk-frontend": "6.1.0",
    "npm-run-all": "4.1.5",
    "plyr": "3.7.2",
    "postcss-loader": "6.2.1",
    "postcss-preset-env": "7.5.0",
    "prettier": "^2.6.2",
    "resolve-url-loader": "5.0.0",
    "rimraf": "^3.0.2",
    "sass": "1.51.0",
    "sass-loader": "12.6.0",
    "svgstore": "3.0.1",
    "webpack": "5.72.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.2",
    "webpack-manifest-plugin": "5.0.0",
    "webpack-remove-empty-scripts": "0.8.0"
  }
}
